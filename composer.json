{
  "name": "saeven/zf3-twig-trans",
  "description": "ZF3 module that glues Twig's {% trans %} and the standard translator together",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/Saeven/zf3-twig-trans",
  "keywords": [
    "zf2",
    "twig",
    "trans",
    "translator",
    "i18n"
  ],
  "authors": [
    {
      "name": "Alexandre Lemaire",
      "email": "alemaire@circlical.com",
      "homepage": "http://circlical.com/",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0",
    "kokspflanze/zfc-twig": "@stable",
    "twig/twig": "^1",
    "twig/extensions": "@stable",
    "zendframework/zend-servicemanager": "@stable",
    "zendframework/zend-mvc-i18n": "@stable",
    "zendframework/zend-mvc": "@stable",
    "zendframework/zend-view": "@stable"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "codacy/coverage": "^1.0"
  },
  "autoload": {
    "psr-0": {
      "CirclicalTwigTrans\\": "src/"
    }
  }
}
